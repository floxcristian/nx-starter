{
  "name": "@nx-starter/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "serve:all": "nx run-many --target=serve --all --parallel",
    "serve:front-admin": "nx serve front-admin",
    "serve:api-users": "nx serve api-users",
    "serve:api-orders": "nx serve api-orders",
    "build:all": "nx run-many -t=build --all --parallel",
    "build:prod": "nx run-many -t=build --all --parallel --configuration=production",
    "build:front-admin": "nx build front-admin --configuration=production",
    "build:api-users": "nx build api-users --configuration=production",
    "build:api-orders": "nx build api-orders --configuration=production",
    "docker:front-admin": "docker build -f apps/front-admin/Dockerfile -t front-admin .",
    "docker:api-users": "docker build . --target production -t api-users --build-arg APP_NAME=api-users --build-arg APP_PORT=3001",
    "docker:api-orders": "docker build . --target production -t api-orders --build-arg APP_NAME=api-orders --build-arg APP_PORT=3002",
    "test:all": "nx run-many --target=test --all",
    "lint:all": "nx run-many -t=lint --all --parallel",
    "lint:fix": "nx run-many -t=lint --all --parallel --fix",
    "gcp:setup": "bash ./tools/scripts/setup-gcp.sh"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~20.2.0",
    "@angular/common": "~20.2.0",
